antechamber -j 4 -fi mol2 -fo mol2 -c gas -at gaff -i FDE.mol2 -o FDE-p.mol2 -pf n -nc 0 -eq 1 -pl 10 -seq no
parmchk2 -i FDE-p.mol2 -o FDE-p.frcmod -f mol2 -s gaff
source leaprc.gaff
mol = loadmol2 FDE-p.mol2
check mol
loadamberparams FDE-p.frcmod
saveamberparm mol FDE-p-tleap.top FDE-p-tleap.crd
quit

cat tleap.in >> log
